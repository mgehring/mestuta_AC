<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script><div id="map_canvas" style="height:300px;width:450px;"></div><script type="text/javascript">window.setTimeout("initGmaps();",300);function initGmaps(){var myOptions = {zoom:15,center:new google.maps.LatLng(50.7770054, 6.080389299999979),mapTypeId: google.maps.MapTypeId.ROADMAP};map = new google.maps.Map(document.getElementById("map_canvas"), myOptions);marker = new google.maps.Marker({map: map, position: new google.maps.LatLng(50.7770054, 6.080389299999979)});infowindow = new google.maps.InfoWindow({content: "<b>Fachschaft Kommunikationswissenschaft</b><br>Eilfschornsteinstraße 15<br>52062 Aachen"});google.maps.event.addListener(marker, "click", function(){infowindow.open(map,marker);});infowindow.open(map,marker);}</script>

